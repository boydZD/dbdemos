{
  "autoscale": {
    "min_workers": 2,
    "max_workers": 4
  },
  "cluster_name": "dbdemos-{{DEMO_NAME}}-{{CURRENT_USER_NAME}}",
  "spark_version": "14.3.x-cpu-ml-scala2.12",
  "data_security_mode": "SINGLE_USER",
  "runtime_engine": "STANDARD",
  "spark_conf": {
    "spark.databricks.dataLineage.enabled": "true",
    "spark.databricks.driver.preferredMavenCentralMirrorUrl": "https://artifacts-read.gkp.jpmchase.net/artifactory/maven/",
    "spark.databricks.driver.disableSparkPackagesResolver": "true",
    "spark.databricks.driver.disableDefaultMavenCentralResolver": "true",
    "spark.databricks.safespark.externalUDF.plan.limit": 20
  },
  "ssh_public_keys": [],
  "custom_tags": {
    "project": "dbdemos",
    "demo": "{{DEMO_NAME}}"
  },
  "spark_env_vars": {
    "PIP_INDEX_URL": "https://artifacts-read.gkp.jpmchase.net/artifactory/api/pypi/pypi/simple"
  },
  "autotermination_minutes": 30,
  "clus4ter_source": "UI",
  "init_scripts": [],
  "runtime_engine": "STANDARD"
}